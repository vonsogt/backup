{
    "name": "vonsogt/backup",
    "description": "Backup extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "backup"],
    "homepage": "https://github.com/vonsogt/backup",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
   "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",        
        "laravel/framework": ">=5.7",
        "spatie/laravel-backup": ">=6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Backup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Backup\\BackupServiceProvider"
            ]
        }
    }
}
